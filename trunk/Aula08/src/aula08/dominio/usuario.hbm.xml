<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping 
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="br.uerj.dinfo.model.vo.UsuarioVO" table="usuarios">
		<id name="id">
			<generator class="identity" />
		</id>

		<property name="login" />
		<property name="senha" />
		<property name="email" />
		<property name="tipo" />		
		<property name="dataCadastro" column="datacadastro" />
		<property name="dataExclusao" column="dataexclusao" />	

	</class>

</hibernate-mapping>
